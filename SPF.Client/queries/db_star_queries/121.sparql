PREFIX  dbpo: <http://dbpedia.org/ontology/>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX  dbprop: <http://dbpedia.org/property/>

SELECT  ?name ?protein
WHERE
  { ?proinsulin  rdf:type         dbpo:Biomolecule ;
              dbprop:name         ?name ;
              dbpo:refseqprotein  ?protein
  }
