PREFIX  foaf: <http://xmlns.com/foaf/0.1/>
PREFIX  onto: <http://dbpedia.org/ontology/>

SELECT  ?pagina ?tipo ?idioma ?propietario ?creador
WHERE
  { ?x  foaf:homepage  ?pagina ;
        onto:type      ?tipo ;
        onto:language  ?idioma ;
        onto:owner     ?propietario ;
        onto:author    ?creador
  }
