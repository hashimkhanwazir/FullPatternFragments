PREFIX  rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX  dbpo: <http://dbpedia.org/ontology/>
PREFIX  rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>

SELECT  ?name ?award ?awardName
WHERE
  { ?person  rdf:type    dbpo:Person ;
             rdfs:label  ?name ;
             dbpo:award  ?award
  }
