PREFIX  xsd:  <http://www.w3.org/2001/XMLSchema#>
PREFIX  grs:  <http://www.georss.org/georss/>
PREFIX  dbpo: <http://dbpedia.org/ontology/>
PREFIX  bif:  <bif:>

SELECT  ?x ?a ?b ?abstract
WHERE
  { ?x        grs:point      ?z ;
              dbpo:abstract  ?abstract ;
              ?a             ?b .
    ?abstract  bif:contains  "pretty"
  }
