PREFIX  skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX  dct:  <http://purl.org/dc/terms/>
PREFIX  dbpr: <http://dbpedia.org/resource/>

SELECT  ?categ ?subcat
WHERE
  { dbpr:Smart_grid
              dct:subject   ?categ .
    ?subcat   skos:broader  ?categ .
    ?subcat2  skos:broader  ?subcat
  }
